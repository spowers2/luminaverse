{
  "expo": {
    "name": "LuminaVerse",
    "slug": "luminaverse",
    "version": "1.1.0",
    "description": "Daily Bible verses with peaceful music and streak tracking. Find inspiration through beautiful scripture in a distraction-free interface.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4a7c7e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.labazine.luminaverse",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4a7c7e"
      },
      "package": "com.labazine.luminaverse",
      "permissions": ["android.permission.SCHEDULE_EXACT_ALARM"]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#4a7c7e"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "owner": "spowers2",
    "extra": {
      "eas": {
        "projectId": "d9de421b-13ad-401f-beca-418b16e8f1b0"
      }
    }
  }
}
